{
  "name": "add-only",
  "displayName": "Add Only",
  "description": "Add only to tests easily using CodeLens",
  "version": "1.1.0",
  "publisher": "ub1que",
  "engines": {
    "vscode": "^1.18.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "keywords": [
    "testrunner",
    "mocha",
    "jest",
    "test",
    "unit"
  ],
  "preview": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ub1que/add-only"
  },
  "icon": "ressources/icon.png",
  "main": "./out/extension",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "title": "AddOnly",
      "properties": {
        "AddOnly.templates": {
          "type": "array",
          "default": [
            {
              "name": "it",
              "insertAfterPriority": [
                "concurrent",
                "it"
              ],
              "decline": [
                "skip"
              ]
            }
          ],
          "description": "Templates in order to control behavior"
        }
      }
    },
    "commands": [
      {
        "command": "add-only.add.only",
        "title": "Add only"
      },
      {
        "command": "add-only.remove.only",
        "title": "Remove only"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc --noUnusedParameters --noUnusedLocals -p ./",
    "watch": "tsc -watch  --noUnusedParameters --noUnusedLocals  -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "format": "prettier --write src/**/*.{ts,json} package.json",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,}": [
      "tslint -c tslint.json",
      "git add"
    ]
  },
  "devDependencies": {
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/parser": "^7.12.11",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-syntax-typescript": "^7.12.1",
    "@babel/traverse": "^7.12.12",
    "@babel/types": "^7.12.12"
  }
}
